<h3>fry_test</h3>
<form action='fry_test' method='POST'>
  <input type='text' name='user[name]' value='name'/>
  <input type='text' name='user[email]' value='email'/>
  <input type='text' name='user[password]' value='password'/>
  <input type='text' name='user[api_token]' value='api_token'/>
  <input type='submit' value='submit'/>
</form>

<hr>

<ul>
  <% @users.each do |user| %>
    <li>
      <%= user.name %>
      <%= user.email %>
      <%= user.password %>
      <%= user.api_token %>
    </li>
  <% end %>
</ul>

<a href="/fry_test">Refresh</a>
